    "dotenv-flow": "^3.1.0",
    "express": "^4.17.1",
    "express-timeout-handler": "^2.2.0",
    "node-fetch": "^2.6.6",
    "puppeteer": "^22.12.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^6.4.1",
    "@stryker-mutator/javascript-mutator": "^4.0.0",
    "@stryker-mutator/jest-runner": "^6.4.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.6.0",
    "fs": "0.0.1-security",
    "jest": "^26.2.2",
    "nodemon": "^2.0.4"
  }
