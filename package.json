{
  "name": "fide-ratings-scraper",
  "version": "1.0.6",
  "description": "FIDE player ratings scraper",
  "repository": "github:xRuiAlves/fide-ratings-scraper",
  "main": "src/main.js",
  "bin": {
    "fide-ratings-scraper": "./bin/index.js"
  },
  "scripts": {
    "start": "node src/api.js",
    "dev": "nodemon src/api.js",
